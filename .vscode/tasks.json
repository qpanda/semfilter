{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "Rust: cargo build - semfilter"
		},
		{
			"type": "cargo",
			"subcommand": "run",
			"group": "build",
			"label": "Rust: cargo run - semfilter"
		},
		{
			"type": "cargo",
			"subcommand": "test",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"options": {
				"env": {
					"RUST_BACKTRACE": "1",
				}
			},
			"label": "Rust: cargo test - semfilter"
		}
	]
}